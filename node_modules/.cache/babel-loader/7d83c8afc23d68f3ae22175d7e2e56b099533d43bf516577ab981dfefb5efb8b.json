{"ast":null,"code":"export default {\n  data() {\n    return {\n      dialogVisible: false,\n      form: {\n        id: undefined,\n        userName: undefined,\n        password: undefined,\n        phonenumber: undefined,\n        email: undefined,\n        sex: undefined,\n        status: \"0\",\n        avatar: \"\"\n      },\n      rules: {}\n    };\n  },\n  methods: {\n    handleClose(done) {\n      this.$confirm(\"确认关闭？\").then(_ => {\n        done();\n      }).catch(_ => {});\n    },\n    // 上传成功回传图片url\n    handleAvatarSuccess(res, file) {\n      this.form.avatar = res.data;\n    },\n    // 上传前看下大小和格式\n    beforeAvatarUpload(file) {\n      const isFormat = file.type === \"image/png\" || file.type === \"image/jpg\" || file.type === \"image/jpeg\";\n      const isLt5M = file.size / 1024 / 1024 < 10;\n      if (!isFormat) {\n        this.$message.error(\"上传头像图片只能是 JPG或PNG 格式!\");\n      }\n      if (!isLt5M) {\n        this.$message.error(\"上传头像图片大小不能超过 5MB!\");\n      }\n      return isFormat && isLt5M;\n    },\n    // form重置\n    reset() {\n      this.form = {\n        id: undefined,\n        userName: undefined,\n        password: undefined,\n        phonenumber: undefined,\n        email: undefined,\n        sex: undefined,\n        status: \"0\",\n        avatar: \"\"\n      };\n      this.resetForm(\"form\");\n    }\n  }\n};","map":{"version":3,"mappings":"AA0DA;EACAA;IACA;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACAC;IAEA;EACA;EACAC;IACAC;MACA,uBACAC;QACAC;MACA,GACAC;IACA;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA,iBACAC,6BACAA,6BACAA;MACA;MAEA;QACA;MACA;MACA;QACA;MACA;MACA;IACA;IACA;IACAC;MACA;QACAjB;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACA;IACA;EACA;AACA","names":["data","dialogVisible","form","id","userName","password","phonenumber","email","sex","status","avatar","rules","methods","handleClose","then","done","catch","handleAvatarSuccess","beforeAvatarUpload","file","reset"],"sourceRoot":"src/views","sources":["User.vue"],"sourcesContent":["<template>\r\n  <div class=\"manage\">\r\n    <el-dialog title=\"提示\" :visible.sync=\"dialogVisible\" width=\"30%\">\r\n      <!-- 表单的内容 -->\r\n      <!-- 图片上传 -->\r\n      <el-form\r\n        :model=\"form\"\r\n        ref=\"form\"\r\n        :rules=\"rules\"\r\n        label-width=\"80px\"\r\n        :inline=\"false\"\r\n        size=\"normal\"\r\n        :limit=\"1\"\r\n      >\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\" :offset=\"0\">\r\n            <el-upload\r\n              class=\"avatar-uploader\"\r\n              action=\"http://localhost:7777/image/upload\"\r\n              :show-file-list=\"false\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n              :before-upload=\"beforeAvatarUpload\"\r\n              multiple\r\n            >\r\n              <img v-if=\"form.avatar\" :src=\"form.avatar\" class=\"avatar\" />\r\n              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n          </el-col>\r\n          <!-- 其他表单数据 -->\r\n          <el-col :span=\"12\" :offset=\"0\">\r\n            <el-input\r\n              v-model=\"form.userName\"\r\n              placeholder=\"输入用户名\"\r\n              size=\"normal\"\r\n              clearable\r\n              prop=\"userName\"\r\n            ></el-input>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"dialogVisible = false\"\r\n          >确 定</el-button\r\n        >\r\n      </span>\r\n    </el-dialog>\r\n    <!-- 新增按钮 -->\r\n    <div class=\"manage-header\">\r\n      <el-button type=\"primary\" size=\"default\" @click=\"dialogVisible = true\"\r\n        >新增用户</el-button\r\n      >\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      form: {\r\n        id: undefined,\r\n        userName: undefined,\r\n        password: undefined,\r\n        phonenumber: undefined,\r\n        email: undefined,\r\n        sex: undefined,\r\n        status: \"0\",\r\n        avatar: \"\",\r\n      },\r\n      rules: {},\r\n      \r\n    };\r\n  },\r\n  methods: {\r\n    handleClose(done) {\r\n      this.$confirm(\"确认关闭？\")\r\n        .then((_) => {\r\n          done();\r\n        })\r\n        .catch((_) => {});\r\n    },\r\n    // 上传成功回传图片url\r\n    handleAvatarSuccess(res, file) {\r\n      this.form.avatar = res.data;\r\n    },\r\n    // 上传前看下大小和格式\r\n    beforeAvatarUpload(file) {\r\n      const isFormat =\r\n        file.type === \"image/png\" ||\r\n        file.type === \"image/jpg\" ||\r\n        file.type === \"image/jpeg\";\r\n      const isLt5M = file.size / 1024 / 1024 < 10;\r\n\r\n      if (!isFormat) {\r\n        this.$message.error(\"上传头像图片只能是 JPG或PNG 格式!\");\r\n      }\r\n      if (!isLt5M) {\r\n        this.$message.error(\"上传头像图片大小不能超过 5MB!\");\r\n      }\r\n      return isFormat && isLt5M;\r\n    },\r\n    // form重置\r\n    reset() {\r\n      this.form = {\r\n        id: undefined,\r\n        userName: undefined,\r\n        password: undefined,\r\n        phonenumber: undefined,\r\n        email: undefined,\r\n        sex: undefined,\r\n        status: \"0\",\r\n        avatar: \"\",\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}