{"ast":null,"code":"import { listProduct, updateProduct, delProduct, addProduct, getProduct } from \"@/api/system/product\";\nexport default {\n  data() {\n    var checkprice = (rule, value, callback) => {\n      if (Number.isInteger(value)) {\n        return callback(new Error(\"必须填数字\"));\n      } else if (value < 0) {\n        return callback(new Error(\"数字必须大于零\"));\n      } else {\n        callback();\n      }\n    };\n    return {\n      // 用户表格数据\n      productList: null,\n      // 总条数\n      total: 0,\n      // 标签名称\n      tiptitle: \"\",\n      queryParams: {\n        pageNum: 1,\n        pageSize: 10,\n        title: undefined\n      },\n      dialogVisible: false,\n      form: {\n        id: undefined,\n        title: undefined,\n        price: undefined,\n        num: undefined,\n        image: \"\"\n      },\n      rules: {\n        title: [{\n          required: true,\n          message: \"商品名称不能为空\",\n          trigger: \"blur\"\n        }, {\n          min: 2,\n          max: 20,\n          message: \"商品名称长度必须介于 2 和 20 之间\",\n          trigger: \"blur\"\n        }],\n        price: [{\n          validator: checkprice,\n          trigger: \"blur\"\n        }],\n        num: [{\n          validator: checkprice,\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  mounted() {\n    this.getList();\n  },\n  methods: {\n    submitForm() {\n      this.$refs['form'].validate(valid => {\n        if (valid) {\n          if (this.form.id !== undefined) {\n            updateProduct(this.form).then(response => {\n              this.$modal.msgSuccess('修改成功');\n              this.dialogVisible = false;\n              this.getList();\n            });\n          } else {\n            addProduct(this.form).then(response => {\n              this.$modal.msgSuccess('新增成功');\n              this.dialogVisible = false;\n              this.getList();\n            });\n          }\n        }\n      });\n    },\n    /** 修改按钮操作 */\n    handleUpdate(row) {\n      this.reset();\n      const id = row.id;\n      this.tiptitle = '修改商品';\n      getProduct(id).then(response => {\n        this.form = response;\n        this.dialogVisible = true;\n      });\n    },\n    // 展示 没数据显示暂无数据\n    show(row, column, cellValue, index) {\n      if (cellValue) {\n        return cellValue;\n      }\n      return \"暂无数据\";\n    },\n    // 获得用户列表信息\n    getList() {\n      listProduct(this.queryParams).then(response => {\n        this.productList = response.rows;\n        this.total = response.total;\n      });\n    },\n    /** 新增用户 */\n    handleAdd() {\n      this.reset();\n      this.dialogVisible = true;\n      this.tiptitle = '新增商品';\n    },\n    cancel() {\n      this.dialogVisible = false;\n      this.reset();\n    },\n    // 上传成功回传图片url\n    handleAvatarSuccess(res, file) {\n      this.form.image = res.data;\n    },\n    // 上传前看下大小和格式\n    beforeAvatarUpload(file) {\n      const isFormat = file.type === \"image/png\" || file.type === \"image/jpg\" || file.type === \"image/jpeg\";\n      const isLt5M = file.size / 1024 / 1024 < 10;\n      if (!isFormat) {\n        this.$message.error(\"上传头像图片只能是 JPG或PNG 格式!\");\n      }\n      if (!isLt5M) {\n        this.$message.error(\"上传头像图片大小不能超过 5MB!\");\n      }\n      return isFormat && isLt5M;\n    },\n    // form重置\n    reset() {\n      this.form = {\n        id: undefined,\n        title: undefined,\n        price: undefined,\n        num: undefined,\n        image: \"\"\n      };\n    }\n  }\n};","map":{"version":3,"mappings":"AAwHA,SACAA,aACAC,eACAC,YACAC,YACAC,kBACA;AACA;EACAC;IACA;MACA;QACA;MACA;QACA;MACA;QACAC;MACA;IACA;IACA;MACA;MACAC;MACA;MACAC;MACA;MACAC;MACAC;QACAC;QACAC;QACAC;MACA;MACAC;MACAC;QACAC;QACAH;QACAI;QACAC;QACAC;MACA;MACAC;QACAP,QACA;UAAAQ;UAAAC;UAAAC;QAAA,GACA;UACAC;UACAC;UACAH;UACAC;QACA,EACA;QACAN;UAAAS;UAAAH;QAAA;QACAL;UAAAQ;UAAAH;QAAA;MACA;IACA;EACA;EACAI;IACA;EACA;EACAC;IACAC;MACA;QACA;UACA;YACA5B;cACA;cACA;cACA;YACA;UACA;YACAE;cACA;cACA;cACA;YACA;UACA;QACA;MACA;IACA;IACA;IACA2B;MACA;MACA;MACA;MACA1B;QACA;QACA;MACA;IACA;IACA;IACA2B;MACA;QACA;MACA;MACA;IACA;IACA;IACAC;MACAhC;QACA;QACA;MACA;IACA;IACA;IACAiC;MACA;MACA;MACA;IAEA;IACAC;MACA;MACA;IACA;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA,iBACAC,6BACAA,6BACAA;MACA;MAEA;QACA;MACA;MACA;QACA;MACA;MACA;IACA;IACA;IACAC;MACA;QACAtB;QACAH;QACAI;QACAC;QACAC;MACA;IACA;EACA;AACA","names":["listProduct","updateProduct","delProduct","addProduct","getProduct","data","callback","productList","total","tiptitle","queryParams","pageNum","pageSize","title","dialogVisible","form","id","price","num","image","rules","required","message","trigger","min","max","validator","mounted","methods","submitForm","handleUpdate","show","getList","handleAdd","cancel","handleAvatarSuccess","beforeAvatarUpload","file","reset"],"sourceRoot":"src/views","sources":["Product.vue"],"sourcesContent":["<template>\r\n  <div class=\"manage\">\r\n    <el-dialog :title=\"tiptitle\" :visible.sync=\"dialogVisible\" width=\"40%\">\r\n      <!-- 表单的内容 -->\r\n      <!-- 图片上传 -->\r\n      <el-form\r\n        :model=\"form\"\r\n        ref=\"form\"\r\n        :rules=\"rules\"\r\n        label-width=\"80px\"\r\n        :inline=\"false\"\r\n        size=\"normal\"\r\n        :limit=\"1\"\r\n      >\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"8\" :offset=\"0\">\r\n            <el-upload\r\n              class=\"avatar-uploader\"\r\n              action=\"http://localhost:7777/image/upload\"\r\n              :show-file-list=\"false\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n              :before-upload=\"beforeAvatarUpload\"\r\n              multiple\r\n            >\r\n              <img v-if=\"form.image\" :src=\"form.image\" class=\"avatar\" />\r\n              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n          </el-col>\r\n          <!-- 其他表单数据 -->\r\n          <el-col :span=\"16\" :offset=\"0\">\r\n            <!-- 用户名 -->\r\n            <el-form-item label=\"商品名\" size=\"normal\" prop=\"title\">\r\n              <el-input\r\n                v-model=\"form.title\"\r\n                placeholder=\"输入商品名称\"\r\n                size=\"normal\"\r\n                clearable\r\n              ></el-input>\r\n            </el-form-item>\r\n            <!-- 价格 -->\r\n            <el-form-item label=\"商品价格\" prop=\"price\">\r\n              <el-input\r\n                v-model=\"form.price\"\r\n                placeholder=\"输入商品价格\"\r\n                size=\"normal\"\r\n                clearable\r\n              ></el-input>\r\n            </el-form-item>\r\n            <!-- 数量 -->\r\n            <el-form-item label=\"商品数量\" prop=\"num\">\r\n              <el-input\r\n                v-model=\"form.num\"\r\n                placeholder=\"输入商品数量\"\r\n                size=\"normal\"\r\n                clearable\r\n              ></el-input>\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <!-- 新增按钮 -->\r\n    <div class=\"manage-header\">\r\n      <div class=\"header-button\">\r\n          <el-button type=\"primary\" size=\"default\" @click=\"handleAdd\">新增商品</el-button>\r\n      </div>\r\n      <div class=\"header-search\">\r\n        <el-input\r\n          v-model=\"queryParams.title\"\r\n          placeholder=\"输入商品名\"\r\n          size=\"normal\"\r\n          clearable\r\n          @change=\"getList()\"\r\n        ></el-input>\r\n        <el-button type=\"primary\" size=\"default\" @click=\"getList()\"\r\n          >搜索</el-button\r\n        >\r\n      </div>\r\n    </div>\r\n    <el-table :data=\"productList\" stripe style=\"width: 100%\">\r\n      <el-table-column prop=\"title\" label=\"商品名\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <span style=\"cursor: pointer\" @click=\"handleUpdate(scope.row)\">{{\r\n            scope.row.title\r\n          }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"price\"\r\n        label=\"商品价格\"\r\n        :formatter=\"show\"\r\n        align=\"center\"\r\n      >\r\n      </el-table-column>\r\n      <el-table-column\r\n        prop=\"num\"\r\n        label=\"商品数量\"\r\n        :formatter=\"show\"\r\n        align=\"center\"\r\n      ></el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n      :page-size.sync=\"queryParams.pageSize\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"total\"\r\n      :page-sizes=\"[10, 20, 30, 40]\"\r\n      :current-page.sync=\"queryParams.pageNum\"\r\n      @current-change=\"getList\"\r\n      @size-change=\"getList\"\r\n    >\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  listProduct,\r\n  updateProduct,\r\n  delProduct,\r\n  addProduct,\r\n  getProduct,\r\n} from \"@/api/system/product\";\r\nexport default {\r\n  data() {\r\n    var checkprice = (rule, value, callback) => {\r\n      if (Number.isInteger(value)) {\r\n        return callback(new Error(\"必须填数字\"));\r\n      } else if (value < 0) {\r\n        return callback(new Error(\"数字必须大于零\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    return {\r\n      // 用户表格数据\r\n      productList: null,\r\n      // 总条数\r\n      total: 0,\r\n      // 标签名称\r\n      tiptitle: \"\",\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        title: undefined,\r\n      },\r\n      dialogVisible: false,\r\n      form: {\r\n        id: undefined,\r\n        title: undefined,\r\n        price: undefined,\r\n        num: undefined,\r\n        image: \"\",\r\n      },\r\n      rules: {\r\n        title: [\r\n          { required: true, message: \"商品名称不能为空\", trigger: \"blur\" },\r\n          {\r\n            min: 2,\r\n            max: 20,\r\n            message: \"商品名称长度必须介于 2 和 20 之间\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        price: [{ validator: checkprice, trigger: \"blur\" }],\r\n        num: [{ validator: checkprice, trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    submitForm(){\r\n      this.$refs['form'].validate((valid) => {\r\n        if (valid) {\r\n          if (this.form.id !== undefined) {\r\n            updateProduct(this.form).then((response) => {\r\n              this.$modal.msgSuccess('修改成功')\r\n              this.dialogVisible = false;\r\n              this.getList()\r\n            })\r\n          } else {\r\n            addProduct(this.form).then((response) => {\r\n              this.$modal.msgSuccess('新增成功')\r\n              this.dialogVisible = false;\r\n              this.getList()\r\n            })\r\n          }\r\n        }\r\n      })\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      const id = row.id;\r\n      this.tiptitle = '修改商品';\r\n      getProduct(id).then((response) => {\r\n        this.form = response;\r\n        this.dialogVisible = true;\r\n      });\r\n    },\r\n    // 展示 没数据显示暂无数据\r\n    show(row, column, cellValue, index) {\r\n      if (cellValue) {\r\n        return cellValue;\r\n      }\r\n      return \"暂无数据\";\r\n    },\r\n    // 获得用户列表信息\r\n    getList() {\r\n      listProduct(this.queryParams).then((response) => {\r\n        this.productList = response.rows;\r\n        this.total = response.total;\r\n      });\r\n    },\r\n    /** 新增用户 */\r\n    handleAdd() {\r\n      this.reset()\r\n      this.dialogVisible = true;\r\n      this.tiptitle = '新增商品'\r\n\r\n    },\r\n    cancel() {\r\n      this.dialogVisible = false;\r\n      this.reset();\r\n    },\r\n    // 上传成功回传图片url\r\n    handleAvatarSuccess(res, file) {\r\n      this.form.image = res.data;\r\n    },\r\n    // 上传前看下大小和格式\r\n    beforeAvatarUpload(file) {\r\n      const isFormat =\r\n        file.type === \"image/png\" ||\r\n        file.type === \"image/jpg\" ||\r\n        file.type === \"image/jpeg\";\r\n      const isLt5M = file.size / 1024 / 1024 < 10;\r\n\r\n      if (!isFormat) {\r\n        this.$message.error(\"上传头像图片只能是 JPG或PNG 格式!\");\r\n      }\r\n      if (!isLt5M) {\r\n        this.$message.error(\"上传头像图片大小不能超过 5MB!\");\r\n      }\r\n      return isFormat && isLt5M;\r\n    },\r\n    // form重置\r\n    reset() {\r\n      this.form = {\r\n        id: undefined,\r\n        title: undefined,\r\n        price: undefined,\r\n        num: undefined,\r\n        image: \"\",\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n.manage-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  width: 98%;\r\n  .header-search{\r\n      display: flex;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}