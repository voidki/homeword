{"ast":null,"code":"export default {\n  data() {\n    return {\n      dialogVisible: false,\n      form: {\n        id: undefined,\n        userName: undefined,\n        password: undefined,\n        phonenumber: undefined,\n        email: undefined,\n        sex: undefined,\n        status: \"0\",\n        avatar: \"\"\n      },\n      rules: {\n        userName: [{\n          required: true,\n          message: \"用户名称不能为空\",\n          trigger: \"blur\"\n        }, {\n          min: 2,\n          max: 20,\n          message: \"用户名称长度必须介于 2 和 20 之间\",\n          trigger: \"blur\"\n        }],\n        password: [{\n          required: true,\n          message: \"用户密码不能为空\",\n          trigger: \"blur\"\n        }, {\n          min: 4,\n          max: 20,\n          message: \"用户密码长度必须介于 4 和 20 之间\",\n          trigger: \"blur\"\n        }],\n        email: [{\n          type: \"email\",\n          message: \"'请输入正确的邮箱地址\",\n          trigger: [\"blur\", \"change\"]\n        }],\n        phonenumber: [{\n          pattern: /^1[3|4|5|6|7|8|9][0-9]\\d{8}$/,\n          message: \"请输入正确的手机号码\",\n          trigger: \"blur\"\n        }]\n      }\n    };\n  },\n  methods: {\n    submitForm() {\n      this.dialogVisible = true;\n      console.log(this.form);\n    },\n    cancel() {\n      this.dialogVisible = false;\n      this.reset();\n    },\n    // 上传成功回传图片url\n    handleAvatarSuccess(res, file) {\n      this.form.avatar = res.data;\n    },\n    // 上传前看下大小和格式\n    beforeAvatarUpload(file) {\n      const isFormat = file.type === \"image/png\" || file.type === \"image/jpg\" || file.type === \"image/jpeg\";\n      const isLt5M = file.size / 1024 / 1024 < 10;\n      if (!isFormat) {\n        this.$message.error(\"上传头像图片只能是 JPG或PNG 格式!\");\n      }\n      if (!isLt5M) {\n        this.$message.error(\"上传头像图片大小不能超过 5MB!\");\n      }\n      return isFormat && isLt5M;\n    },\n    // form重置\n    reset() {\n      this.form = {\n        id: undefined,\n        userName: undefined,\n        password: undefined,\n        phonenumber: undefined,\n        email: undefined,\n        sex: undefined,\n        status: \"0\",\n        avatar: \"\"\n      };\n      this.resetForm(\"form\");\n    }\n  }\n};","map":{"version":3,"mappings":"AAkGA;EACAA;IACA;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACAC;QACAP,WACA;UAAAQ;UAAAC;UAAAC;QAAA,GACA;UACAC;UACAC;UACAH;UACAC;QACA,EACA;QACAT,WACA;UAAAO;UAAAC;UAAAC;QAAA,GACA;UACAC;UACAC;UACAH;UACAC;QACA,EACA;QACAP,QACA;UACAU;UACAJ;UACAC;QACA,EACA;QACAR,cACA;UACAY;UACAL;UACAC;QACA;MAEA;IACA;EACA;EACAK;IACAC;MACA;MACAC;IACA;IACAC;MACA;MACA;IACA;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA,iBACAC,6BACAA,6BACAA;MACA;MAEA;QACA;MACA;MACA;QACA;MACA;MACA;IACA;IACA;IACAC;MACA;QACAvB;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MACA;MACA;IACA;EACA;AACA","names":["data","dialogVisible","form","id","userName","password","phonenumber","email","sex","status","avatar","rules","required","message","trigger","min","max","type","pattern","methods","submitForm","console","cancel","handleAvatarSuccess","beforeAvatarUpload","file","reset"],"sourceRoot":"src/views","sources":["User.vue"],"sourcesContent":["<template>\r\n  <div class=\"manage\">\r\n    <el-dialog title=\"提示\" :visible.sync=\"dialogVisible\" width=\"40%\">\r\n      <!-- 表单的内容 -->\r\n      <!-- 图片上传 -->\r\n      <el-form\r\n        :model=\"form\"\r\n        ref=\"form\"\r\n        :rules=\"rules\"\r\n        label-width=\"80px\"\r\n        :inline=\"false\"\r\n        size=\"normal\"\r\n        :limit=\"1\"\r\n      >\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"8\" :offset=\"0\">\r\n            <el-upload\r\n              class=\"avatar-uploader\"\r\n              action=\"http://localhost:7777/image/upload\"\r\n              :show-file-list=\"false\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n              :before-upload=\"beforeAvatarUpload\"\r\n              multiple\r\n            >\r\n              <img v-if=\"form.avatar\" :src=\"form.avatar\" class=\"avatar\" />\r\n              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n          </el-col>\r\n          <!-- 其他表单数据 -->\r\n          <el-col :span=\"16\" :offset=\"0\">\r\n            <!-- 用户名 -->\r\n            <el-form-item label=\"用户名\" size=\"normal\" prop=\"userName\">\r\n              <el-input\r\n                v-model=\"form.userName\"\r\n                placeholder=\"输入用户名\"\r\n                size=\"normal\"\r\n                clearable\r\n              ></el-input>\r\n            </el-form-item>\r\n            <!-- 密码 -->\r\n            <el-form-item label=\"密码\" size=\"normal\" prop=\"password\">\r\n              <el-input\r\n                v-model=\"form.password\"\r\n                placeholder=\"密码\"\r\n                size=\"normal\"\r\n                clearable\r\n              ></el-input>\r\n            </el-form-item>\r\n            <!-- 性别 -->\r\n            <el-form-item label=\"用户性别\">\r\n              <el-select v-model=\"form.sex\" placeholder=\"请选择\">\r\n                <el-option :key=\"'0'\" label=\"男\" :value=\"'0'\" />\r\n                <el-option :key=\"'1'\" label=\"女\" :value=\"'1'\" />\r\n                <el-option :key=\"'2'\" label=\"未知\" :value=\"'2'\" />\r\n              </el-select>\r\n            </el-form-item>\r\n            <!-- 状态 -->\r\n            <el-form-item label=\"状态\">\r\n              <el-radio-group v-model=\"form.status\">\r\n                <el-radio :key=\"'0'\" :label=\"'0'\">正常</el-radio>\r\n                <el-radio :key=\"'1'\" :label=\"'1'\">停用</el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"手机号码\" prop=\"phonenumber\">\r\n              <el-input\r\n                v-model=\"form.phonenumber\"\r\n                placeholder=\"请输入手机号码\"\r\n                maxlength=\"11\"\r\n              />\r\n            </el-form-item>\r\n            <el-form-item label=\"邮箱\" prop=\"email\">\r\n              <el-input\r\n                v-model=\"form.email\"\r\n                placeholder=\"请输入邮箱\"\r\n                maxlength=\"50\"\r\n              />\r\n            </el-form-item>\r\n          </el-col>\r\n        </el-row>\r\n      </el-form>\r\n\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitForm\"\r\n          >确 定</el-button\r\n        >\r\n      </span>\r\n    </el-dialog>\r\n    <!-- 新增按钮 -->\r\n    <div class=\"manage-header\">\r\n      <el-button type=\"primary\" size=\"default\" @click=\"this.dialogVisible = true\"\r\n        >新增用户</el-button\r\n      >\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      dialogVisible: false,\r\n      form: {\r\n        id: undefined,\r\n        userName: undefined,\r\n        password: undefined,\r\n        phonenumber: undefined,\r\n        email: undefined,\r\n        sex: undefined,\r\n        status: \"0\",\r\n        avatar: \"\",\r\n      },\r\n      rules: {\r\n        userName: [\r\n          { required: true, message: \"用户名称不能为空\", trigger: \"blur\" },\r\n          {\r\n            min: 2,\r\n            max: 20,\r\n            message: \"用户名称长度必须介于 2 和 20 之间\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        password: [\r\n          { required: true, message: \"用户密码不能为空\", trigger: \"blur\" },\r\n          {\r\n            min: 4,\r\n            max: 20,\r\n            message: \"用户密码长度必须介于 4 和 20 之间\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        email: [\r\n          {\r\n            type: \"email\",\r\n            message: \"'请输入正确的邮箱地址\",\r\n            trigger: [\"blur\", \"change\"],\r\n          },\r\n        ],\r\n        phonenumber: [\r\n          {\r\n            pattern: /^1[3|4|5|6|7|8|9][0-9]\\d{8}$/,\r\n            message: \"请输入正确的手机号码\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm(){\r\n        this.dialogVisible = true\r\n        console.log(this.form);\r\n    },\r\n    cancel(){\r\n      this.dialogVisible = false\r\n      this.reset();\r\n    },\r\n    // 上传成功回传图片url\r\n    handleAvatarSuccess(res, file) {\r\n      this.form.avatar = res.data;\r\n    },\r\n    // 上传前看下大小和格式\r\n    beforeAvatarUpload(file) {\r\n      const isFormat =\r\n        file.type === \"image/png\" ||\r\n        file.type === \"image/jpg\" ||\r\n        file.type === \"image/jpeg\";\r\n      const isLt5M = file.size / 1024 / 1024 < 10;\r\n\r\n      if (!isFormat) {\r\n        this.$message.error(\"上传头像图片只能是 JPG或PNG 格式!\");\r\n      }\r\n      if (!isLt5M) {\r\n        this.$message.error(\"上传头像图片大小不能超过 5MB!\");\r\n      }\r\n      return isFormat && isLt5M;\r\n    },\r\n    // form重置\r\n    reset() {\r\n      this.form = {\r\n        id: undefined,\r\n        userName: undefined,\r\n        password: undefined,\r\n        phonenumber: undefined,\r\n        email: undefined,\r\n        sex: undefined,\r\n        status: \"0\",\r\n        avatar: \"\",\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}